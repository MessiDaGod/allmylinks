@inject IJSRuntime JS;
@using Microsoft.JSInterop;


<iframe id="sqlformat" src="https://messidagod.github.io/allmylinks/DemoPage.html" width="70%" height="60%"></iframe>

@code {

    bool ready;
    string Url { get; set; } = string.Empty;
    private static readonly HttpClient http = new();
    // 1
    protected override async Task OnInitializedAsync()
    {
        Url = await JS.InvokeAsync<string>("AML.geturl");
        await base.OnInitializedAsync();
    }

    private string Followers { get; set; } = "0";
    // 2
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            ready = true;
            StateHasChanged();
        }
        await base.OnAfterRenderAsync(firstRender);
    }
}

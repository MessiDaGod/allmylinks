    <_ContentIncludedByDefault Remove="artifacts\obj\Stl.CommandR\project.assets.json" />
    <_ContentIncludedByDefault Remove="artifacts\obj\Stl.CommandR\Stl.CommandR.csproj.nuget.dgspec.json" />
